{
    "name": "masterflash-ru/kontakt",
    "description": "Страница с контактами для системы управления Simba мультиязычная",
    "type": "Service",
    "license": "BSD-3-Clause",
    "keywords": [
        "masterflash",
        "simba",
        "laminas"
    ],
    "homepage": "https://www.masterflash.ru/",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2 || ^8.0",
        "laminas/laminas-cache": "*",
        "laminas/laminas-mvc": "*",
        "laminas/laminas-serializer": "*",
        "laminas/laminas-router": "*",
        "laminas/laminas-inputfilter": "*",
        "laminas/laminas-i18n": "*",
        "masterflash-ru/validator_i18n": "*",
        "masterflash-ru/statpage": "*",
        "masterflash-ru/emailer": "*",
        "laminas/laminas-form": "*",
        "laminas/laminas-captcha": "*",
        "laminas/laminas-recaptcha": "*",
        "laminas/laminas-mvc-plugin-prg": "*"
    },
    "autoload": {
        "psr-4": {
            "Mf\\Kontakt\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mf\\Kontakt\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev",
            "dev-develop": "2.0-dev"
        },
        "zf": {
            "component": "Mf\\Kontakt",
            "config-provider": "masterflash-ru\\Mf\\Kontakt\\ConfigProvider"
        }
    }
}
