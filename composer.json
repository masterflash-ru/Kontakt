{
    "name": "masterflash-ru/kontakt",
    "description": "Страница с контактами для системы управления Simba мультиязычная",
    "type": "Service",
    "license": "BSD-3-Clause",
    "keywords": [
        "masterflash",
        "simba",
        "laminas"
    ],
    "homepage": "https://www.masterflash.ru/",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2",
        "laminas/laminas-cache": "^2.7",
        "laminas/laminas-mvc": "^3.0.0",
        "laminas/laminas-serializer": "^2.5",
        "laminas/laminas-router": "^3.0",
        "laminas/laminas-inputfilter": "^2.7",
        "laminas/laminas-i18n": "^2.7",
        "masterflash-ru/validator_i18n": "^1.0",
        "masterflash-ru/statpage": "^2.0",
        "masterflash-ru/emailer": "*",
        "laminas/laminas-form": "^2.10",
        "laminas/laminas-captcha": "^2.7",
        "laminas/laminas-recaptcha": "^3.2",
        "laminas/laminas-mvc-plugin-prg": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Mf\\Kontakt\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mf\\Kontakt\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev",
            "dev-develop": "1.1-dev"
        },
        "zf": {
            "component": "Mf\\Kontakt",
            "config-provider": "masterflash-ru\\Mf\\Kontakt\\ConfigProvider"
        }
    }
}
